                }
            ]
        },
        {
            "description": "request to update a book, but isbn in path variable could not be found",
            "request": {
                "method": "PUT",
                "path": "/books/0",
                "headers": {
                    "Content-Type": "application/json;charset=UTF-8"
                },
                "body": {
                    "author": "Robert Cecil Martin",
                    "isbn": "9780132350883",
                    "publisher": "Prentice Hall",
                    "title": "Clean Code"
                }
            },
            "response": {
                "status": 404,
                "body": "Isbn not found"
            },
            "providerStates": [
                {
                    "name": "updateBookWithNoMatchingIsbn"
                }
            ]
        },
        {
            "description": "request to update a book",
            "request": {
                "method": "PUT",
                "path": "/books/123456789",
                "headers": {
                    "Content-Type": "application/json;charset=UTF-8"
                },
                "body": {
                    "author": "Robert Cecil Martin",
                    "isbn": "9780132350883",
                    "publisher": "Prentice Hall",
                    "title": "Clean Code"
                }
            },
            "response": {
                "status": 200
            },
            "providerStates": [
                {
                    "name": "updateBook"
                }
            ]
        }
    ],
    "metadata": {
        "pactSpecification": {
            "version": "3.0.0"
        },
        "pact-jvm": {
            "version": "3.5.24"
        }
    }
}