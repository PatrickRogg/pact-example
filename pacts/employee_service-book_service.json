                            "combine": "AND"
                        },
                        "$.publisher": {
                            "matchers": [
                                {
                                    "match": "type"
                                }
                            ],
                            "combine": "AND"
                        },
                        "$.title": {
                            "matchers": [
                                {
                                    "match": "type"
                                }
                            ],
                            "combine": "AND"
                        }
                    }
                }
            },
            "providerStates": [
                {
                    "name": "getBookByIsbn"
                }
            ]
        },
        {
            "description": "request for book with specific book title",
            "request": {
                "method": "GET",
                "path": "/books/search",
                "query": {
                    "bookTitle": [
                        "Clean"
                    ]
                }
            },
            "response": {
                "status": 200,
                "headers": {
                    "Content-Type": "application/json;charset=UTF-8"
                },
                "body": [
                    {
                        "author": "Robert Cecil Martin",
                        "isbn": "9780132350884",
                        "publisher": "Prentice Hall",
                        "title": "Clean Code"
                    }
                ],
                "matchingRules": {
                    "body": {
                        "$": {
                            "matchers": [
                                {
                                    "match": "type",
                                    "min": 0
                                }
                            ],
                            "combine": "AND"
                        },
                        "$[*].author": {
                            "matchers": [
                                {
                                    "match": "type"
                                }
                            ],
                            "combine": "AND"
                        },
                        "$[*].isbn": {
                            "matchers": [
                                {
                                    "match": "type"
                                }
                            ],
                            "combine": "AND"
                        },
                        "$[*].publisher": {
                            "matchers": [
                                {
                                    "match": "type"
                                }
                            ],
                            "combine": "AND"
                        },
                        "$[*].title": {
                            "matchers": [
                                {
                                    "match": "type"
                                }
                            ],
                            "combine": "AND"
                        }
                    }
                }
            },
            "providerStates": [
                {
                    "name": "searchBookByBookTitle"
                }
            ]
        },
        {
            "description": "request to update a book with no isbn value set",
            "request": {
                "method": "PUT",
                "path": "/books/123456789",
                "headers": {
                    "Content-Type": "application/json;charset=UTF-8"
                },
                "body": {
                    "author": "Robert Cecil Martin",
