{
    "provider": {
        "name": "book_service"
    },
    "consumer": {
        "name": "employee_service"
    },
    "interactions": [
        {
            "description": "request to create a new book with no isbn value set",
            "request": {
                "method": "POST",
                "path": "/books",
                "headers": {
                    "Content-Type": "application/json;charset=UTF-8"
                },
                "body": {
                    "author": "Robert Cecil Martin",
                    "isbn": null,
                    "publisher": "Prentice Hall",
                    "title": "Clean Code"
                }
            },
            "response": {
                "status": 400,
                "body": "Isbn can not be null"
            },
            "providerStates": [
                {
                    "name": "createBookWithNoIsbn"
                }
            ]
        },
        {
            "description": "request to create a new book",
            "request": {
                "method": "POST",
                "path": "/books",
                "headers": {
                    "Content-Type": "application/json;charset=UTF-8"
                },
                "body": {
                    "author": "Robert Cecil Martin",
                    "isbn": "9780132350884",
                    "publisher": "Prentice Hall",
                    "title": "Clean Code"
                }
            },
            "response": {
                "status": 201,
                "headers": {
                    "Content-Type": "application/json;charset=UTF-8"
                },
                "body": {
                    "author": "Robert Cecil Martin",
                    "isbn": "9780132350884",
                    "publisher": "Prentice Hall",
                    "title": "Clean Code"
                },
                "matchingRules": {
                    "body": {
                        "$.author": {
                            "matchers": [
                                {
                                    "match": "type"
                                }
                            ],
                            "combine": "AND"
                        },
                        "$.isbn": {
                            "matchers": [
                                {
                                    "match": "type"
                                }
                            ],
                            "combine": "AND"
                        },
                        "$.publisher": {
                            "matchers": [
                                {
                                    "match": "type"
                                }
                            ],
                            "combine": "AND"
                        },
                        "$.title": {
                            "matchers": [
                                {
                                    "match": "type"
                                }
                            ],
                            "combine": "AND"
                        }
                    }
                }
            },
            "providerStates": [
                {
                    "name": "createBook"
                }
            ]
        },
        {
            "description": "request to delete a book",
            "request": {
                "method": "DELETE",
                "path": "/books/123456789"
            },
            "response": {
                "status": 200
            },
            "providerStates": [
                {
                    "name": "deleteBookBy"
                }
            ]
        },
        {
            "description": "request to delete a book, but isbn in path variable could not be found",
            "request": {
                "method": "DELETE",
                "path": "/books/0"
            },
            "response": {
                "status": 404,
                "body": "Isbn not found"
            },
            "providerStates": [
                {
                    "name": "deleteBookWithNoMatchingIsbn"
                }
            ]
        },
        {
            "description": "request for all books",
            "request": {
                "method": "GET",
                "path": "/books"
            },
            "response": {
                "status": 200,
                "headers": {
                    "Content-Type": "application/json;charset=UTF-8"
                },
                "body": [
                    {
                        "author": "Robert Cecil Martin",
                        "isbn": "9780132350884",
                        "publisher": "Prentice Hall",
                        "title": "Clean Code"
                    }
                ],
                "matchingRules": {
                    "body": {
                        "$": {
                            "matchers": [
                                {
                                    "match": "type",
                                    "min": 0
                                }
                            ],
                            "combine": "AND"
                        },
                        "$[*].author": {
                            "matchers": [
                                {
                                    "match": "type"
                                }
                            ],
                            "combine": "AND"
                        },
                        "$[*].isbn": {
                            "matchers": [
                                {
                                    "match": "type"
                                }
                            ],
                            "combine": "AND"
                        },
                        "$[*].publisher": {
                            "matchers": [
                                {
                                    "match": "type"
                                }
                            ],
                            "combine": "AND"
                        },
                        "$[*].title": {
                            "matchers": [
                                {
                                    "match": "type"
                                }
                            ],
                            "combine": "AND"
                        }
                    }
                }
            },
            "providerStates": [
                {
                    "name": "getAllBooks"
                }
            ]
        },
        {
            "description": "request for book with specific isbn, but isbn in path variable could not be found",
            "request": {
                "method": "GET",
                "path": "/books/0"
            },
            "response": {
                "status": 404,
                "body": "Isbn not found"
            },
            "providerStates": [
                {
                    "name": "getBookByIsbnWithNoMatchingIsbn"
                }
            ]
        },
        {
