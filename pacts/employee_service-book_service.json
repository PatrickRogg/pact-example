                    "Content-Type": "application/json;charset=UTF-8"
                },
                "body": {
                    "author": "Robert Cecil Martin",
                    "isbn": "9780132350884",
                    "publisher": "Prentice Hall",
                    "title": "Clean Code"
                },
                "matchingRules": {
                    "body": {
                        "$.author": {
                            "matchers": [
                                {
                                    "match": "type"
                                }
                            ],
                            "combine": "AND"
                        },
                        "$.isbn": {
                            "matchers": [
                                {
                                    "match": "type"
                                }
